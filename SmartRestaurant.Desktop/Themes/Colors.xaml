<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Free Table -->
    <SolidColorBrush x:Key="FreeTableBackground" Color="#E0FCEB" />
    <SolidColorBrush x:Key="FreeTableBorderBrush" Color="#A5D6A7" />
    <SolidColorBrush x:Key="FreeTableForeground" Color="#388E3C" />

    <!-- Busy Table -->
    <SolidColorBrush x:Key="BusyTableBackground" Color="#FFCDD2" />
    <SolidColorBrush x:Key="BusyTableBorderBrush" Color="#EF9A9A" />
    <SolidColorBrush x:Key="BusyTableForeground" Color="DarkRed" />

    <!-- Reserved Table -->
    <SolidColorBrush x:Key="ReservedTableBackground" Color="#FFF9C4" />
    <SolidColorBrush x:Key="ReservedTableBorderBrush" Color="#FFF176" />
    <SolidColorBrush x:Key="ReservedTableForeground" Color="DarkOrange" />

    <!-- Selected Table -->
    <SolidColorBrush x:Key="SelectedTableBackground" Color="#FFF3E0" />
    <SolidColorBrush x:Key="SelectedTableBorderBrush" Color="#FFA726" />
    <DropShadowEffect x:Key="SelectedTableShadow" Color="Black" BlurRadius="5" ShadowDepth="2" Opacity="0.15"/>

    <SolidColorBrush x:Key="FirstColor" Color="#EEEEEE"/>
    <SolidColorBrush x:Key="SecondColor" Color="White"/>
    <SolidColorBrush x:Key="TheerdColor" Color="#A59C9C"/>
    <SolidColorBrush x:Key="FourthColor" Color="#d6d6d6d6"/>
    <SolidColorBrush x:Key="FifthColor" Color="#0FBC5F"/>
    <SolidColorBrush x:Key="SixthColor" Color="#D41509"/>
    <SolidColorBrush x:Key="SeventhColor" Color="#25D409"/>

    <SolidColorBrush x:Key="BorderColor" Color="#383838"/>
    <SolidColorBrush x:Key="LabelColor" Color="White"/>
    <SolidColorBrush x:Key="labelColor" Color="#0D0D0D"/>

    <SolidColorBrush x:Key="btncolor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="btnMouseEnter" Color="#F3E0E0"/>
    <SolidColorBrush x:Key="btnMouseClick" Color="#C0B1B1"/>

    <SolidColorBrush x:Key="Create_Btn_Color" Color="#329DFF"/>
    <SolidColorBrush x:Key="Create_Btn_MouseEnter" Color="#57AEFF"/>
    <SolidColorBrush x:Key="Create_Btn_MouseClick" Color="#2696FF"/>
    <SolidColorBrush x:Key="Border_Mouse_Enter" Color="#57AEFF"/>

    <SolidColorBrush x:Key="BFirstColor" Color="#04C217"/>
    <SolidColorBrush x:Key="BSecondColor" Color="#0000F7"/>
    <SolidColorBrush x:Key="BSecondColorr" Color="#2B3D59"/>

    <SolidColorBrush x:Key="BTheerdColor" Color="#D30000"/>

    <SolidColorBrush x:Key="SwitchOffBrush" Color="#E0E0E0"/>
    <SolidColorBrush x:Key="SwitchOnBrush" Color="#4CAF50"/>


</ResourceDictionary>